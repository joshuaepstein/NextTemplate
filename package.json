{
  "name": "report-generator",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"email dev -p 3333\" \"prisma studio --browser none\"",
    "dev:web": "next dev",
    "dev:email": "email dev -p 3333",
    "dev:studio": "prisma studio --browser none",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "script": "tsx ./scripts/run.ts",
    "email:dev": "email dev",
    "email:build": "email build",
    "postinstall": "prisma generate",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "update-prisma": "pnpm i --save-dev prisma@latest && pnpm i @prisma/client@latest"
  },
